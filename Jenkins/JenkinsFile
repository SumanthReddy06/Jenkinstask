pipeline {
    agent any
    environment {
		DOCKERHUB_CREDENTIALS=credentials('Docker')
	}

    stages {
        stage('Build and Deploy') {
            steps {

                
                sh 'sudo docker build -t sumanth1206/helloworld:v1 .'
		        sh 'sudo chmod 666 /var/run/docker.sock'
                sh 'docker push sumanth1206/helloworld:v1'
        }      
        }
        
    }
}